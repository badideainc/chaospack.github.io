					},
					{
						"queue_command": {
							"command": [
								"fill ~1 ~-2 ~1 ~-1 ~2 ~-1 light_block_10 replace air"
							]
						}
					}
				]
			},
			"minecraft:start_death": {
				"queue_command": {
					"command": [
						"fill ~5 ~4 ~5 ~-5 ~-4 ~-5 air replace light_block_10"
					]
				}
			}
