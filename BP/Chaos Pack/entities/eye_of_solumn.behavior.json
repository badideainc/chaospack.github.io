{
	"format_version": "1.16.0",
	"minecraft:entity": {
		"description": {
			"identifier": "chaospack:eye_of_solumn",
			"runtime_identifier": "minecraft:ghast",
			"is_spawnable": false,
			"is_summonable": true,
			"is_experimental": false
		},
		"component_groups": {
			"scale_small": {
				"minecraft:scale": {
					"value": 0.5
				}
			},
			"scale_normal": {
				"minecraft:scale": {
					"value": 0.75
				}
			},

			"scale_large": {
				"minecraft:scale": {
					"value": 1
				}
			}
		},
		"components": {
			"minecraft:is_hidden_when_invisible": {
			},
			"minecraft:experience_reward": {
				"on_death": "query.last_hit_by_player ? 5 + (query.equipment_count * Math.Random(1,3)) : 0"
			},
			"minecraft:nameable": {
			},
			"minecraft:type_family": {
				"family": [ "star", "monster", "mob" ]
			},
			"minecraft:equipment": {
				"table": "loot_tables/entities/star.json"
			},
			"minecraft:health": {
				"value": 20,
				"max": 20
			},
			"minecraft:attack": {
				"damage": 5
			},
			"minecraft:collision_box": {
				"width": 4.02,
				"height": 4
			},
			"minecraft:movement": {
				"value": 1.0
			},
			"minecraft:navigation.walk": {
				"can_path_over_water": true
			},
			"minecraft:movement.basic": {

			},
			"minecraft:jump.static": {
			},
			"minecraft:can_climb": {
			},
			"minecraft:fire_immune": {
			},
			"minecraft:despawn": {
				"despawn_from_distance": {}
			},
			"minecraft:behavior.float": {
				"priority": 0
			},
			"minecraft:behavior.charge_attack": {
				"priority": 4
			},
			"minecraft:behavior.nearest_attackable_target": {
				"priority": 3,
				"entity_types": [
					{
						"filters": {
							"any_of": [
								{
									"test": "is_family",
									"subject": "other",
									"value": "player"
								},
								{
									"test": "is_family",
									"subject": "other",
									"value": "irongolem"
								}
							]
						},
						"max_dist": 70
					}
				],
				"must_see": true
			},
			"minecraft:behavior.look_at_player": {
				"priority": 9,
				"look_distance": 6.0,
				"probability": 0.02
			},
			"minecraft:behavior.look_at_entity": {
				"priority": 9,
				"look_distance": 6.0,
				"probability": 0.02,
				"filters": {
					"test": "is_family",
					"subject": "other",
					"value": "mob"
				}
			},
			"minecraft:behavior.hurt_by_target": {
				"priority": 1
			},
			"minecraft:physics": {
				"has_gravity": false,
				"has_collision": true
			},
			"minecraft:conditional_bandwidth_optimization": {
			},
			"minecraft:game_event_movement_tracking": {
				"emit_move": false,
				"emit_swim": false
			}
		},
		"events": {
			"minecraft:entity_spawned": {
				"randomize": [
					{
						"weight": 30,
						"add": {
							"component_groups": [ "scale_small" ]
						}
					},
					{
						"weight": 15,
						"add": {
							"component_groups": [ "scale_normal" ]
						}
					},
					{
						"weight": 50,
						"add": {
							"component_groups": [ "scale_large" ]
						}
					}
				]
			}
		}
	}
}