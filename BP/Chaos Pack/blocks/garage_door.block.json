			},
			{
				"condition": "q.block_state('minecraft:cardinal_direction') == 'east' && q.block_state('chaospack:open') ",
				"components": {
					"minecraft:transformation": {
						"rotation": [ 0, 90, 90 ],
						"translation": [ 0, 0.3, 0 ]
					}
